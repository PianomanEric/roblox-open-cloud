{
  "name": "@pianomaneric/roblox-open-cloud",
  "version": "1.0.0",
  "description": "A TypeScript package to simplify the use of Roblox's Open Cloud API.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest --silent=false --config jest.config.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PianomanEric/roblox-open-cloud.git"
  },
  "author": "Eric Falk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PianomanEric/roblox-open-cloud/issues"
  },
  "homepage": "https://github.com/PianomanEric/roblox-open-cloud#readme",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsdoc": "^46.4.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.4.0"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ]
}
